<?xml version="1.0" encoding="utf-8" ?>
<Configuration>
    
  <Minimum_Event_Duration_In_ms>50</Minimum_Event_Duration_In_ms>
  
  <Maximum_Event_Duration_In_ms>3000</Maximum_Event_Duration_In_ms>
  
  <Minimum_Number_Of_Fixations_For_Regression>2</Minimum_Number_Of_Fixations_For_Regression>
    <Minimum_Duration_Of_Fixation_For_Regression>150</Minimum_Duration_Of_Fixation_For_Regression>
  
  <Minimum_Number_Of_Fixations_For_First_Pass>2</Minimum_Number_Of_Fixations_For_First_Pass>
  <Minimum_Duration_Of_Fixation_For_First_Pass>150</Minimum_Duration_Of_Fixation_For_First_Pass>
  
  <Minimum_Number_Of_Fixations_For_Skip>2</Minimum_Number_Of_Fixations_For_Skip>
  <Minimum_Duration_Of_Fixation_For_Skip>150</Minimum_Duration_Of_Fixation_For_Skip>
  
  <Number_Of_Fixations_In_AOI_For_Exception>4</Number_Of_Fixations_In_AOI_For_Exception>
  
  <Number_Of_Fixations_Out_Of_AOI_For_Exception>1</Number_Of_Fixations_Out_Of_AOI_For_Exception>
  
  <!-- Second_File_Filtering_denominator :enter the value-denominator-  with nothing = 0 ,Mean_AOI_Size = 1,Mean_AOI_Coverage = 2-->
  <Second_File_Filtering_denominator>0</Second_File_Filtering_denominator>

  <!--
    Analyze_Extent:
      Preview = 1,
      Full = 2
  -->
  <Analyze_Extent>2</Analyze_Extent>
  
  <!--
  Preview_Limit:
	Fixed_Time_In_sec = 1,
	Percentage_of_Participant_Time = 2
  -->
  <Preview_Limit>1</Preview_Limit>
	<Fixed_Time_In_sec>800.4</Fixed_Time_In_sec>
	<Percentage_of_Participant_Time>20</Percentage_of_Participant_Time>

  <!--
    Remove_Fixations_Appeared_Before_First_AOI:
      No = 1,
      First Page = 2,
	  Every Page = 3
  -->
  <Remove_Fixations_Appeared_Before_First_AOI>1</Remove_Fixations_Appeared_Before_First_AOI>
  

  <Standard_Deviation>2.5</Standard_Deviation>
  
  <OutputFiles>
    <First_Excel_File_Name>Fixation</First_Excel_File_Name>
    <Second_Excel_File_Name>AOI</Second_Excel_File_Name>
    <Considered_Second_Excel_File_Name>AOI - Filtered</Considered_Second_Excel_File_Name>
    <Third_Excel_File_Name>Text</Third_Excel_File_Name>

    <Logs_Excel_File_Name>Logs</Logs_Excel_File_Name>
    <Excel_Files_Extension>.xlsx</Excel_Files_Extension>
  </OutputFiles>
  
  <DealingWithExceptions>
    <Dealing_With_Exceptions_Limit_In_Pixels>20</Dealing_With_Exceptions_Limit_In_Pixels>


    <!--
      Dealing_With_Exceptions_Inside_The_Limit:
        Do_Nothing = 1,
        Skip_In_First_Pass = 2,
        Change_AOI_Group = 3
    -->
    <Dealing_With_Exceptions_Inside_The_Limit>3</Dealing_With_Exceptions_Inside_The_Limit>
    
    
    
    <!--
      Dealing_With_Exceptions_Outside_The_Limit:  
        Do_Nothing = 1,
        Skip_In_First_Pass = 2
    -->
    <Dealing_With_Exceptions_Outside_The_Limit>2</Dealing_With_Exceptions_Outside_The_Limit>  
  </DealingWithExceptions>
  
</Configuration>


