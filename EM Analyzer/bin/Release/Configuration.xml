<?xml version="1.0" encoding="utf-8" ?>
<Configuration>
  
  <Minimum_Event_Duration_In_ms>50</Minimum_Event_Duration_In_ms>
  <Maximum_Event_Duration_In_ms>1000</Maximum_Event_Duration_In_ms>
  <Minimum_Number_Of_Fixations_In_A_Regression>2</Minimum_Number_Of_Fixations_In_A_Regression>
  <Number_Of_Fixations_In_AOI_For_Exception>2</Number_Of_Fixations_In_AOI_For_Exception>
  <Number_Of_Fixations_Out_Of_AOI_For_Exception>3</Number_Of_Fixations_Out_Of_AOI_For_Exception>


  <!--
    Remove_Fixations_Appeared_Before_First_AOI:
      No = 0,
      Yes = 1
  -->
  <Remove_Fixations_Appeared_Before_First_AOI>1</Remove_Fixations_Appeared_Before_First_AOI>
  <Minimum_Event_Duration_In_For_Skip_In_ms>3000</Minimum_Event_Duration_In_For_Skip_In_ms>

  <Standard_Deviation>1</Standard_Deviation>
  
  <OutputFiles>
    <First_Excel_File_Name>First</First_Excel_File_Name>
    <Second_Excel_File_Name>Second</Second_Excel_File_Name>
    <Considered_Second_Excel_File_Name>Second - Considered</Considered_Second_Excel_File_Name>
    <Third_Excel_File_Name>Third</Third_Excel_File_Name>

    <Logs_Excel_File_Name>Logs</Logs_Excel_File_Name>
    <Excel_Files_Extension>.xlsx</Excel_Files_Extension>
  </OutputFiles>
  
  <DealingWithExceptions>
    <Dealing_With_Exceptions_Limit_In_Pixels>20</Dealing_With_Exceptions_Limit_In_Pixels>


    <!--
      Dealing_With_Exceptions_Inside_The_Limit:
        Do_Nothing = 1,
        Skip_In_First_Pass = 2,
        Change_AOI_Group = 3
    -->
    <Dealing_With_Exceptions_Inside_The_Limit>3</Dealing_With_Exceptions_Inside_The_Limit>
    
    
    
    <!--
      Dealing_With_Exceptions_Outside_The_Limit:  
        Do_Nothing = 1,
        Skip_In_First_Pass = 2
    -->
    <Dealing_With_Exceptions_Outside_The_Limit>1</Dealing_With_Exceptions_Outside_The_Limit>  
  </DealingWithExceptions>
  
</Configuration>
